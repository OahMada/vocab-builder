{
	"name": "vocab-builder",
	"version": "0.1.0",
	"private": true,
	"scripts": {
		"dev": "next dev",
		"build": "next build",
		"start": "next start -p 4000",
		"lint": "next lint",
		"postinstall": "prisma generate",
		"new-component": "new-component -l ts",
		"prisma-push": "dotenv -e .env.local -- npx prisma db push",
