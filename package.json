		"@radix-ui/react-popover": "^1.1.2",
		"@radix-ui/react-toast": "^1.2.2",
		"@types/js-cookie": "^3.0.6",
		"@types/node": "^22.9.0",
		"@types/react": "^18.3.12",
		"@types/react-dom": "^18.3.1",
		"axios": "^1.7.7",
		"eslint": "^9.14.0",
		"eslint-config-next": "^15.0.3",
		"html-react-parser": "^5.1.18",
		"immer": "^10.1.1",
		"js-cookie": "^3.0.5",
		"new-component": "^5.0.3",
